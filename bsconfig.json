{
  "name": "scrollock",
  "namespace": true,
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-webapi",
    "rescript-body-scroll-lock"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Dom": "Dom"
    },
    "importPath": "relative",
    "debug": {
      "all": false
    }
  }
}